LOGFILE=/dev/null

PHP_RELEASE=3
APACHE_RELEASE=2
REDIS_RELEASE=2

[ -z $APACHE_VERSION ] && APACHE_VERSION=2.4.23
[ -z $PHP_VERSION ] && PHP_VERSION=5.6.25
[ -z $REDIS_VERSION ] && REDIS_VERSION=3.2.3
[ -z $DISTRO ] && DISTRO=centos-6

export LOGFILE REDIS_RELEASE APACHE_RELEASE PHP_RELEASE APACHE_VERSION PHP_VERSION REDIS_VERSION DISTRO
